                        <div class="d-flex product_overview flex-column p-2 justify-content-center text-center mx-1">
                            <div class="product_overview_pic mx-auto"> 
                                <img class="product_overview_pic p-1" src="{{this.itemPic}}" alt={{this.itemName}}>
                            </div>
                            
                            <h6 class="p-1">{{this.itemName}}</h6>
                            <h6 class="p-1">₱ {{this.itemPrice}}</h6>
                        </div>
                    </a>
                {{/each}}
            </div>

