                <div class="col justify-content-center text-align-center">
                    <h1 class="home_labels">TRENDING ITEMS</h1>
                </div>
            </div>

            <div class="d-flex overview_cont p-2 flex-row flex-wrap mx-auto">
                {{#each item}}
